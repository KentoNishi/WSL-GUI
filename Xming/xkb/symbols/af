   key <AD09> { [ Arabic_khah, 0x1000681, 0x1000027] };
   key <AD10> { [ Arabic_hah, 0x1000685, 0x1000022] };
   key <AD11> { [ Arabic_jeem, 0x100005d, 0x100007d ] };
   key <AD12> { [ 0x1000686, 0x100005b, 0x100007b ] };

   key <AC01> { [ Arabic_sheen, 0x100069a ] };
