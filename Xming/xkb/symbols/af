// 2006-02-15 file created by M. Emal Alekozai <memala@gmx.net>

partial alphanumeric_keys 
xkb_symbols "ps" {
   name[Group1]= "Afghanistan - Pashto";

   key <TLDE> { [ 0x100200d, 0x10000f7, dead_tilde ] };
